<!-- Definition of Circabc User Model. It extends the cm:person contentModel -->
<model name="su:surveymodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <!-- Optional meta-data about the model -->
   <description>Survey Model</description>
   <author>Matthieu Sprunck</author>
   <version>1.0</version>

   <!-- Imports are required to allow references to definitions in other models -->
   <imports>
      <!-- Import Alfresco Content Domain Model Definitions -->
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
   </imports>

   <!-- Introduction of new namespaces defined by this model -->
   <namespaces>
      <namespace uri="http://www.cc.cec/circabc/model/survey/1.0" prefix="su"/>
   </namespaces>

   <types>
      <type name="su:surveys">
         <parent>cm:folder</parent>
      </type>
   </types>
</model>
