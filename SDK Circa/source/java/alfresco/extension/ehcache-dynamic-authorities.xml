<ehcache>
	<!-- defaults -->
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="15" timeToLiveSeconds="15" overflowToDisk="false">
	</defaultCache>

	<cache name="org.alfresco.dynamic.authority.lockOwnerDynamicAuthority"
		maxElementsInMemory="500" eternal="false" timeToLiveSeconds="2"
		overflowToDisk="false">

	</cache>

	<cache name="org.alfresco.dynamic.authority.ownerDynamicAuthority"
		maxElementsInMemory="500" eternal="false" timeToLiveSeconds="2"
		overflowToDisk="false">

	</cache>

	<cache
		name="eu.cec.digit.circabc.service.dynamic.authority.circabcDynamicAuthority"
		maxElementsInMemory="50000" eternal="false" timeToLiveSeconds="15"
		overflowToDisk="false">
	</cache>



</ehcache>
