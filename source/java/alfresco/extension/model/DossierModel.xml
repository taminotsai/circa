<!-- Definition of Dossier Model. It extends the cm:folder contentModel -->
<model name="do:dossiermodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <!-- Optional meta-data about the model -->
   <description>Dossier Model</description>
   <author>Slobodan Filipovic</author>
   <version>1.0</version>

   <!-- Imports are required to allow references to definitions in other models -->
   <imports>
      <!-- Import Alfresco Content Domain Model Definitions -->
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
   </imports>

   <!-- Introduction of new namespaces defined by this model -->
   <namespaces>
      <namespace uri="http://www.cc.cec/circabc/model/dossier/1.0" prefix="do"/>
   </namespaces>

   <types>
      <type name="do:dossier">
        <parent>cm:folder</parent>
   	 </type>
  </types>
</model>
