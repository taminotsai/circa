<alfresco-config area="space-contents">

    <!--
        Define all available navgation options.
    -->
    <config evaluator="string-compare" condition="Circabc Root Customization">
    	<!-- Representation of the root node -->
    	<root>
	    	<!-- Here the navigation preference customization -->
	    	<space name="navigation">
		    	<space name="information">
			    	<space name="container">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2" >classpath:alfresco/extension/customisation/navigation/information/container/default.xml</file>
			    	</space>
			    	<space name="content">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/information/content/default.xml</file>
			    	</space>
		    	</space>
				<space name="library">
			    	<space name="container">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/library/container/default.xml</file>
			    	</space>
			    	<space name="content">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/library/content/default.xml</file>
			    	</space>
			    	<space name="discussion">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/library/discussion/default.xml</file>
			    	</space>
				</space>
		    	<space name="newsgroup">
			    	<space name="forum">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/newsgroup/forum/default.xml</file>
			    	</space>
			    	<space name="topic">
				    	<file name="default.xml" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/navigation/newsgroup/topic/default.xml</file>
			    	</space>
		    	</space>
	    	</space>

	    	<!-- Here the templates preference customization -->
	    	<space name="templates">
				<space name="mails">
			    	<space name="applyForMembership">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/templates/mails/applyForMembership/default.ftl</file>
			    	</space>
			    	<space name="eventCreateNotification">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="4">classpath:alfresco/extension/customisation/templates/mails/eventCreateNotification/default.ftl</file>
			    	</space>
			    	<space name="eventDeleteNotification">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/templates/mails/eventDeleteNotification/default.ftl</file>
			    	</space>
			    	<space name="eventReminder">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="3" >classpath:alfresco/extension/customisation/templates/mails/eventReminder/default.ftl</file>
			    	</space>
			    	<space name="eventUpdateNotification">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/templates/mails/eventUpdateNotification/default.ftl</file>
			    	</space>
			    	<space name="includes">
				    	<file name="propertyList.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="10">classpath:alfresco/extension/customisation/templates/mails/includes/propertyList.ftl</file>
			    	</space>
			    	<space name="inviteUser">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/templates/mails/inviteUser/default.ftl</file>
			    	</space>
			    	<space name="mailMeContent">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/mailMeContent/default.ftl</file>
			    	</space>
			    	<space name="mailMeDossier">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/mailMeDossier/default.ftl</file>
			    	</space>
			    	<space name="meetingReminder">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/meetingReminder/default.ftl</file>
			    	</space>
			    	<space name="notifyDoc">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="true" revison="1">classpath:alfresco/extension/customisation/templates/mails/notifyDoc/default.ftl</file>
			    	</space>
			    	<space name="notifyPost">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/notifyPost/default.ftl</file>
			    	</space>
			    	<space name="refuseApplication">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/refuseApplication/default.ftl</file>
			    	</space>
			    	<space name="resendOtherPwd">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/resendOtherPwd/default.ftl</file>
			    	</space>
			    	<space name="resendOwnPwd">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/resendOwnPwd/default.ftl</file>
			    	</space>
			    	<space name="selfRegistration">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/selfRegistration/default.ftl</file>
			    	</space>
			    	<space name="sendToMembers">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/sendToMembers/default.ftl</file>
			    	</space>
			    	<space name="shareSpace">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/shareSpace/default.ftl</file>
			    	</space>
			    	<space name="rejectPost">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/rejectPost/default.ftl</file>
			    	</space>
			    	<space name="signalAbuse">
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/signalAbuse/default.ftl</file>
			    	</space>
			    	<space name="autoUploadSuccess">
				    	<file name="success.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/autoUpload/success.ftl</file>
				    	
			    	</space>
			    	<space name="autoUploadError">
				    	
				    	<file name="error.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/autoUpload/error.ftl</file>
			    	</space>
			    	<space name="autoUploadFtpProblem">
				    	
				    	<file name="ftp-problem.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/autoUpload/ftp-problem.ftl</file>
			    	</space>
			    	<space name="supportRequest">
				    	
				    	<file name="default.ftl" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/templates/mails/supportRequest/default.ftl</file>
			    	</space>
		    	</space>
	    	</space>

	    	<!-- Here the icons preference customization -->
	    	<space name="iglookAndFeel">
				<space name="icon">
			    	<space name="definition">
				    	<file name="config.properties" versionable="true" editOnline="true" forceUpdate="false" revison="1">classpath:alfresco/extension/customisation/iglookAndFeel/icon/definition/config.properties</file>
			    	</space>
					<space name="images">
				    	<file name="circabc_logo.gif" versionable="true" editOnline="false" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/iglookAndFeel/icon/images/circabc_logo.gif</file>
			    	</space>
		    	</space>
	    	</space>

	    	<!-- Here the mail disclamer preference customization -->
	    	<space name="templates">
				<space name="disclamer">
			    	<space name="logo">
				    	<file name="circabc_logo.gif" versionable="true" editOnline="false" forceUpdate="false" revison="2">classpath:alfresco/extension/customisation/templates/disclamer/logo/circabc_logo.gif</file>
			    	</space>
		    	</space>
	    	</space>

	    	<!-- Here the person preference customization -->
	    	<space name="users">
				<space name="preferences">
			    	<space name="avatar">
				    	<file name="default_avatar.png" versionable="true" editOnline="false" forceUpdate="false" revison="2" guestPerm="Read" registredPerm="Read" >classpath:alfresco/extension/customisation/users/preferences/avatar/default_avatar.png</file>
			    	</space>
		    	</space>
	    	</space>
	    	
	    	<!-- Here the helpdesk emails list -->
	    	<space name="helpdesk">
				<space name="contact">
				    	<file name="contact-helpdesk.properties" versionable="true" editOnline="true" forceUpdate="false" revison="1" guestPerm="Read" registredPerm="Read" >classpath:alfresco/extension/customisation/contact-helpdesk.properties</file>
		    	</space>
	    	</space>
	    </root>
    </config>
</alfresco-config>


